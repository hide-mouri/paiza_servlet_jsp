<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
	<display-name>paiza_servlet_jsp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>Hello</servlet-name>
		<servlet-class>HelloServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>RpgMenu</servlet-name>
		<servlet-class>Menu</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RpgMenu</servlet-name>
		<url-pattern>/menu</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RpgWalk</servlet-name>
		<servlet-class>Walk</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RpgWalk</servlet-name>
		<url-pattern>/walk</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RpgAttack</servlet-name>
		<servlet-class>Attack</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RpgAttack</servlet-name>
		<url-pattern>/attack</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RpgSayHello</servlet-name>
		<servlet-class>SayHello</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RpgSayHello</servlet-name>
		<url-pattern>/sayHello</url-pattern>
	</servlet-mapping>
</web-app>